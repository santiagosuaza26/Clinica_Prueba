          "// Se ejecuta antes de cada request",
          "",
          "// Si tienes un token de autenticación, configúralo aquí",
          "// pm.request.headers.add({key: 'Authorization', value: 'Bearer ' + pm.variables.get('authToken')});",
          "",
          "// Ejemplo de configuración automática de headers basada en el rol",
          "const role = pm.variables.get('userRole');",
          "if (role) {",
          "    pm.request.headers.add({key: 'Role', value: role});",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Script de test para validar respuestas",
          "// Se ejecuta después de cada response",
          "",
          "// Ejemplo: Si la respuesta es de autenticación, guardar el token",
          "if (pm.response.code === 200 && pm.request.url.toString().includes('/authenticate')) {",
          "    const response = pm.response.json();",
          "    if (response && response.token) {",
          "        pm.variables.set('authToken', response.token);",
          "    }",
          "}",
          "",
          "// Validar códigos de estado comunes",
          "pm.test('Status code is not 500', function () {",
          "    pm.response.to.not.have.status(500);",
          "});",
          "",
          "// Mostrar información útil en consola",
          "console.log('Request URL:', pm.request.url.toString());",
          "console.log('Response Status:', pm.response.code);",
          "console.log('Response Time:', pm.response.responseTime, 'ms');"
        ]
      }
